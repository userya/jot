<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:projects="http://www.jot.org/projects/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ecore:nsPrefix="projects" ecore:package="jot.model.projects" targetNamespace="http://www.jot.org/projects/1.0">
  <xsd:import namespace="http://www.eclipse.org/emf/2002/Ecore" schemaLocation="platform:/plugin/org.eclipse.emf.ecore/model/Ecore.xsd"/>
  <xsd:element ecore:ignore="true" name="Projects" type="projects:Projects"/>
  <xsd:element ecore:ignore="true" name="Project" type="projects:Project"/>
  <xsd:complexType name="Projects">
    <xsd:sequence>
      <xsd:element ecore:resolveProxies="true" maxOccurs="unbounded" minOccurs="0" name="project" type="projects:Project"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Project">
    <xsd:attribute ecore:id="true" name="name" type="ecore:EString" use="required"/>
    <xsd:attribute name="resource" type="ecore:EString" use="required"/>
    <xsd:attribute name="namespace" type="ecore:EString" use="required"/>
  </xsd:complexType>
</xsd:schema>
